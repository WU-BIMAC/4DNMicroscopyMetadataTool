      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="AnnotationIDKey">
      <xsd:selector xpath="OME:StructuredAnnotations/*"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>

