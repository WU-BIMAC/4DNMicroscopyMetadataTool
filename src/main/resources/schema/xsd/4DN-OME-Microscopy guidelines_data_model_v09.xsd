      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ROIIDKey">
      <xsd:selector xpath="OME:ROI"/>
      <xsd:field xpath="@ID"/>
    </xsd:key>
    <xsd:key name="ShapeIDKey">
      <xsd:selector
